{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".svn",
    "node_modules/*",
    "src"
  ],
  "verbose": true,
  "execMap": {
    "js": "node"
  },
  "watch": [
  "bin/config/*", "bin/controller/*","bin/model/*","bin/service/*","app.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}